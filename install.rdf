<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>helloworld@zotero.org</em:id>
		<em:name>Evernote-Zotero</em:name>
		<em:version>1.0.0.1</em:version>
		<em:description>evernote-zotero integration</em:description>
		<em:creator>Bill Yuan</em:creator>
		<em:updateURL>https://www.zotero.org/download/plugins.rdf</em:updateURL>
		<em:type>2</em:type> <!-- type=extension -->

		<!-- Requires Firefox or Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>5.0.0</em:minVersion>
				<em:maxVersion>5.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:targetApplication>
			<Description>
				<em:id>juris-m@juris-m.github.io</em:id>
				<em:minVersion>4.999</em:minVersion>
				<em:maxVersion>5.*</em:maxVersion>
			</Description>
    	</em:targetApplication>

		<em:localized>
			<Description>
				<em:locale>en-US</em:locale>
				<em:name>Evernote-Zotero</em:name>
				<em:creator>Bill Yuan</em:creator>
				<em:description>evernote-zotero integration</em:description>
			</Description>
    	</em:localized>
	</Description>
</RDF>
